ic designer with the ability to learn and collaborate in fast-paced environments. Equipped with a diverse and promising skillset. Proficient in many technologies and passionate about beautiful and accessible web design.\n                </p>\n            </div>\n        </div>\n    )\n}","/Users/melissa/Desktop/Coding-Bootcamp/Homework/React-Portfolio/React-Portfolio/Portfolio/src/components/Contact/index.js",["50"],"import React, { useState } from 'react';\nimport {\n  Box,\n} from '@chakra-ui/react';\nimport './style.css';\n\nexport default function Contact() {\n  const [name, setName] = useState('');\n  const [email, setEmail] = useState('');\n  const [message, setMessage] = useState('');\n\n  const handleInputChange = (e) => {\n    const { target } = e;\n    const inputType = target.name;\n    const inputValue = target.value;\n\n    if (inputType === 'name') {\n      setName(inputValue);\n    }\n\n    if (inputType === 'email') {\n      setEmail(inputValue);\n    }\n\n    if (inputType === 'message') {\n      setMessage(inputValue)\n    }\n  }\n\n  const handleFormSubmit = (e) => {\n    e.preventDefault();\n\n    setName('');\n    setEmail('');\n    setMessage('');\n  }\n\n  return (\n    <div className='container'>\n      <p className='title is-2'>\n        Contact\n      </p>\n\n      <form className='form'>\n        <div className='field'>\n          <label className='label is-size-4'>\n            Name\n          </label>\n\n          <div className='control'>\n            <input \n              className={name ? 'input' : 'input is-danger'} \n              type='text' \n              name='name'\n              value={name}\n              onChange={handleInputChange} \n            />\n          </div>\n\n          <p className={name ? 'is-hidden help' : 'help is-danger'}>\n            Please enter a name\n          </p>\n        </div>\n\n        <div className='field'>\n          <label className='label is-size-4'>\n            Email\n          </label>\n\n          <div className='control has-icons-left has-icons-right'>\n            <input \n              type='text' \n              name='email'\n              value={email}\n              onChange={handleInputChange} \n            />\n          </div>\n        </div>\n\n        <div className='field'>\n          <label className='label is-size-4'>\n            Message\n          </label>\n          \n          <div className='control'>\n            <textarea \n              className={message ? 'textarea' : 'textarea is-danger'}\n              name='message'\n              value={message}\n              onChange={handleInputChange} \n            /> \n          </div>\n\n          <p className={message? 'is-hidden help' : 'help is-danger'}>\n            Message required\n          </p>\n        </div>\n\n        <div className='field is-grouped is-grouped-right'>\n          <div className='control is-right'>\n            <button \n              className='button is-link' \n              onClick={handleFormSubmit}\n            >\n              Submit\n            </button>\n          </div>\n        </div>\n      </form>\n    </div>\n  );\n}",{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":3,"column":10,"nodeType":"57","messageId":"58","endLine":3,"endColumn":15},{"ruleId":"55","severity":1,"message":"59","line":6,"column":8,"nodeType":"57","messageId":"58","endLine":6,"endColumn":16},{"ruleId":"55","severity":1,"message":"60","line":3,"column":5,"nodeType":"57","messageId":"58","endLine":3,"endColumn":8},{"ruleId":"61","severity":1,"message":"62","line":9,"column":13,"nodeType":"63","endLine":9,"endColumn":16},{"ruleId":"55","severity":1,"message":"64","line":9,"column":5,"nodeType":"57","messageId":"58","endLine":9,"endColumn":9},{"ruleId":"55","severity":1,"message":"65","line":10,"column":5,"nodeType":"57","messageId":"58","endLine":10,"endColumn":10},{"ruleId":"55","severity":1,"message":"66","